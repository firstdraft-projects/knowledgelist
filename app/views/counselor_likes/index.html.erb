<div class="page-header">
  <h1>
    Counselor likes
    <a href="/counselor_likes/new" class="btn btn-lg btn-success">New Counselor like</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Want</th>
        <th>Counselor</th>
        <th>Like</th>
        <th>Actions</th>
      </tr>

      <% @counselor_likes.each do |counselor_like| %>
      <tr>
        <td><%= counselor_like.want_id %></td>
        <td><%= counselor_like.counselor_id %></td>
        <td><%= counselor_like.like %></td>
        <td>
          <a href="/counselor_likes/<%= counselor_like.id %>" class="btn btn-primary">Show</a>
          <a href="/counselor_likes/<%= counselor_like.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_counselor_like/<%= counselor_like.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
