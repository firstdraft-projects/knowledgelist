<div class="page-header">
  <h3>Counselor Like #<%= @counselor_like.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Want</dt>
      <dd>
        <% if @counselor_like.want.present? %>
          <a href="/wants/<%= @counselor_like.want_id %>">
            <%= @counselor_like.want.user_id %>
          </a>
        <% end %>
      </dd>

      <dt>Counselor</dt>
      <dd>
        <% if @counselor_like.counselor.present? %>
          <a href="/users/<%= @counselor_like.counselor_id %>">
            <%= @counselor_like.counselor.created_at %>
          </a>
        <% end %>
      </dd>

      <dt>Like</dt>
      <dd><%= @counselor_like.like %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/counselor_likes" class="btn btn-primary">
        Back
      </a>
      <a href="/counselor_likes/<%= @counselor_like.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_counselor_like/<%= @counselor_like.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
